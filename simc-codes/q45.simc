//To find composite factors of a number
MAIN
    //Take input of the number whose factors are to be calculated
    var n = input("Input number : ", "i")
    print("Composite Factors of {n} are:")
    var i = 1
    var p = 1
    while(i <= n){
	if (n % i == 0){
	    var j = 2
	    while (j < i) {
		if (i%j == 0){
		    p=0
		}
	    j++
	    }
	    if(p == 0){
		print(" {i}")
	    }
	}
    i++
    }
END_MAIN
